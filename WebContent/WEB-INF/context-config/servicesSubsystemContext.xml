	<bean id="listDao" class="com.podosoftware.community.list.dao.jdbc.JdbcListDao" parent="jdbcDaoSupport"  />
	
	<bean id="listService" class="com.podosoftware.community.list.service.impl.ListServiceImpl"  
		p:memberCache-ref="memberCache"
		p:listDao-ref="listDao"  />
		
